{
  "name": "extension",
  "version": "1.0.0",
  "description": "This is a browser extension to scrape LinkedIn profiles and export them as JSON for SuperpoweredCV analysis. It supports both Google Chrome and Mozilla Firefox.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,json,html,css}\"",
    "package": "npm run lint && npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.2.5"
  }
}
